            getSpeakerData: {
                PLAYBACK_TIMBRE: 'https://cdn.dxomark.com/dakdata/graph/speaker/{speakerId}/playback/timbre',
                PLAYBACK_VOLUME_STEPS: 'https://cdn.dxomark.com/dakdata/graph/speaker/{speakerId}/playback/volume/steps'
            }
        }
    </script>
